<form id="extraFieldsForm" action="/existing_user/addEntry" method="post">
    
    <h3 class="new-measurement-title">New Measurement</h3>

    <div class="input-group">
        <label for="date">Date:</label>
        <input type="date" id="date" name="date" required>
    </div>
    <div class="input-group">
        <label for="food">Food:</label>
        <input type="text" id="food" name="food" required>
    </div>
    <div class="input-group">
        <label for="sugarLevelAfterTwoHours">Sugar Level After Two Hours:</label>
        <input type="number" id="sugarLevelAfterTwoHours" name="sugarLevelAfterTwoHours" required>
    </div>
    <button type="submit">Submit Entry</button>
</form>



